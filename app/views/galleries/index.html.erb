


  <% @galleries.each do |gallery| %>
      <% artistid = gallery.artist_id %>
      <% @artist = Artist.find(artistid) %>
      <% @profile = Profile.find(artistid) %>
        <h3 id='galleries-index'><%= link_to "#{gallery.gallery_name}", artist_gallery_path(@artist.id, gallery.id) %></h3>
        <h4 id='gallery-index-artist'><%="by #{@profile.name}"%></h4>
  <% end %>
